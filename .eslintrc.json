{
  "env": {
    "browser": true,
    "es2021": true
  },
  // "extends": ["airbnb", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["off", { "devDependencies": true }],
    "react/jsx-props-no-spreading": "off",
    "no-nested-ternary": "off"
  }
}
